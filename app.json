{
  "expo": {
    "sdkVersion": "24.0.0",
    "version": "1.0",
    "name": "Challenge",
    "description": "App for training your memory",
    "slug": "cards",
    "platforms": ["ios", "android"],
    "orientation": "portrait",
    "primaryColor": "#2793e8",
    "privacy": "public",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "notification": {
      "icon": "./assets/images/notification-icon.png",
      "color": "#2793e8"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.apk.sakal"
    },
    "androidShowExponentNotificationInShellApp": true,
    "android": {
      "package": "com.apk.sakal"
    },
    "assetBundlePatterns": [
      "assets/**",
      "node_modules/react-navigation/src/**/*.png",
      "node_modules/@expo/vector-icons/fonts/*.ttf"
    ]
  }
}